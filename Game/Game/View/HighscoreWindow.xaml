<Window x:Class="Game.View.HighscoreWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game.View"
        mc:Ignorable="d"
        Title="HighscoreWindow" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <DataGrid AutoGenerateColumns="False" x:Name="table" HorizontalAlignment="Left" Height="221" Margin="10,10,0,0" VerticalAlignment="Top" Width="100">
            <DataGrid.Columns >
                <DataGridTextColumn Binding="{Binding Path=Name}" MinWidth="50" />
                <DataGridTextColumn Binding="{Binding Path=Score}" MinWidth="50" />
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
